<html lang="en">
    <head>
      <title>Shopping Cart</title>
      <meta charset="utf-8">
      <meta name="viewport" content="width=device-width, initial-scale=1">
      <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
       <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
      <%= stylesheet_link_tag 'user' %>
      <%= stylesheet_link_tag  'bootstrap_css/bootstrap.min.css' %>
      <%= javascript_include_tag 'bootstrap_js/jquery.min.js' %>
      <%= javascript_include_tag 'bootstrap_js/bootstrap.min.js' %>

    </head>
    <body>
      <div class="nav-bar">
          <%= render template: 'layouts/partials/_navbar' %>
      </div>
      <div class="main-body">
        <%= yield %> 
      </div>    
    </body>
</html>
